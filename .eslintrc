{
  "parser": "babel-eslint",
  "plugins": [],
  "extends": ["airbnb", "prettier"],
  "env": {
    "es6": true,
    "jest": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "RandomListNode": true,
    "UndirectedGraphNode": true,
    "ListNode": true,
    "TreeNode": true
  },
  "rules": {
    "no-cond-assign": 0,
    "no-useless-constructor": 0,
    "no-empty-function": 0,
    "prefer-destructuring": 0,
    "guard-for-in": 0,
    "no-plusplus": 0,
    "arrow-body-style": 0,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "react/jsx-filename-extension": 0,
    "no-var": 0,
    "no-unused-vars": 0,
    "func-names": 0,
    "no-else-return": 0,
    "prefer-const": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "operator-assignment": 0,
    "no-continue": 0,
    "import/prefer-default-export": 0,
    "radix": 0,
    "yoda": 0,
    "no-restricted-syntax": 0,
    "no-use-before-define": 0,
    "no-lonely-if": 0,
    "no-bitwise": 0,
    "vars-on-top": 0,
    "no-nested-ternary": 0,
    "class-methods-use-this": 0,
    "prefer-template": 0,
    "no-multi-assign": 0,
    "no-loop-func": 0,
    "one-var": 0,
    "no-console": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module": {
        "extensions": ["*", ".js", ".jsx"]
      }
    }
  }
}
